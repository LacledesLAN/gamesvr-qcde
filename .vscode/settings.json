{
    "files.encoding": "utf8",
    "terminal.integrated.scrollback": 4000,
    "[markdown]": {
        "cSpell.fixSpellingWithRenameProvider": true,
        "cSpell.advanced.feature.useReferenceProviderWithRename": true,
        "cSpell.advanced.feature.useReferenceProviderRemove": "/^#+\\s/",
        "editor.rulers": [120],
        "editor.unicodeHighlight.ambiguousCharacters": true,
        "editor.unicodeHighlight.invisibleCharacters": true,
        "editor.wordWrap": "on"
    },
    "[shellscript]": {
        "editor.rulers": [100],
        "files.eol": "\n"
    },
    "cSpell.words": [
        "BUILDNODE",
        "Deathmatch",
        "dockerhub",
        "Dockerized",
        "DOOMWADDIR",
        "Freedoom",
        "freeplay",
        "Gamemode",
        "gamesvr",
        "hostemail",
        "Instagib",
        "iwad",
        "Laclede's",
        "lacledeslan",
        "optfile",
        "QCDE",
        "railgun",
        "respawn",
        "TLMS",
        "trixie",
        "XPOST",
        "Zandronum"
    ]
}
